import java.util.Scanner;

class numeropositivo {
    public static void main(String []args){
        Scanner sc = new Scanner(System.in);
        int i, cont=0;
        float val;

        for(i=1;i<=6;i++){
            val = sc.nextFloat();
            if (val > 0.0){
                cont++;

            }
        }
        sc.close();
        System.out.printf("%d valores positivos\n", cont);
    }   
}
